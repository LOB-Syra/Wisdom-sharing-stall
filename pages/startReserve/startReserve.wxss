.info {
  width: 100%;
  height: 80rpx;
  border-top: 1rpx solid #eee;
  border-bottom: 1rpx solid #eee;
  padding: 30rpx 30rpx;
  position: relative;
}

.stallName {
  font-size: 35rpx;
  font-weight: 600;
  margin-bottom: 10rpx;
}

.stallTime {
  color: #aaa;
  font-size: 27rpx;
}

.seatDemos {
  color: #aaa;
  margin: 0 auto;
  width: 80%;
  height: 70rpx;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
}

.seatDemo image {
  width: 45rpx;
  height: 45rpx;
}

.seatDemoItem {
  white-space: nowrap;
  width: 45rpx;
}