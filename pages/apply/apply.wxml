<van-cell is-link title="申请资质" link-type="navigateTo" url="/pages/index/index" />
<van-notify id="van-notify" />
<van-collapse value="{{ activeNames }}" bind:change="showApply">
  <van-collapse-item title="我的申请">
    <block wx:if="{{ applyed }}">
      <van-cell-group>
        <van-cell title="品牌" value="{{brand}}" />
        <van-cell title="服务类型" value="{{type}}" />
        <van-cell title="负责人姓名" value="{{name}}" />
        <van-cell title="身份证号" value="{{idcard}}" />
        <van-cell title="联系方式" value="{{tel}}" />
        <van-cell title="提交时间" value="{{createTime}}" />
        <van-cell title="申请状态" value="{{condition}}" />
      </van-cell-group>
    </block>
  </van-collapse-item>
</van-collapse>