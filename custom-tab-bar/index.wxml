<!--custom-tab-bar/index.wxml-->
<van-tabbar active="{{ active }}" bind:change="onChange">
  <van-tabbar-item icon="search">
    <image slot="icon" src="{{ icon.normal }}" mode="aspectFit" style="width: 40px; height: 18px;" />
    <image slot="icon-active" src="{{ icon.active }}" mode="aspectFit" style="width: 40px; height: 18px;" /> 搜索
  </van-tabbar-item>

  <van-tabbar-item icon="add-o">
    <image slot="icon" src="{{ icon.normal }}" mode="aspectFit" style="width: 40px; height: 18px;" />
    <image slot="icon-active" src="{{ icon.active }}" mode="aspectFit" style="width: 40px; height: 18px;" /> 发布</van-tabbar-item>
  <van-tabbar-item icon="manager-o">
    <image slot="icon" src="{{ icon.normal }}" mode="aspectFit" style="width: 40px; height: 18px;" />
    <image slot="icon-active" src="{{ icon.active }}" mode="aspectFit" style="width: 40px; height: 18px;" /> 我的</van-tabbar-item>


</van-tabbar>